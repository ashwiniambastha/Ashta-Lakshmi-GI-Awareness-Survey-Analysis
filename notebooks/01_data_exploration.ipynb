    "## 4. Statistical Analysis"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Initialize statistical analyzer\n",
    "stat_analyzer = StatisticalAnalyzer(config)\n",
    "\n",
    "# Perform statistical analysis\n",
    "stat_results = stat_analyzer.perform_analysis(cleaned_data)\n",
    "\n",
    "# Display key findings\n",
    "print(\"KEY STATISTICAL FINDINGS\")\n",
    "print(\"=\" * 50)\n",
    "\n",
    "# Descriptive statistics\n",
    "print(\"\\n1. DESCRIPTIVE STATISTICS:\")\n",
    "print(\"-\" * 30)\n",
    "desc_stats = stat_results['descriptive_stats']\n",
    "print(f\"Average Age: {desc_stats['Age']['mean']:.1f} years\")\n",
    "print(f\"Average Experience: {desc_stats['Years_of_Experience']['mean']:.1f} years\")\n",
    "print(f\"GI Awareness Rate: {desc_stats['GI_Aware']['percentages']['Yes']:.1f}%\")\n",
    "print(f\"E-commerce Usage Rate: {desc_stats['Uses_Ecommerce']['percentages']['Yes']:.1f}%\")\n",
    "\n",
    "# Chi-square test results\n",
    "if 'chi_square_tests' in stat_results:\n",
    "    print(\"\\n2. CHI-SQUARE TEST RESULTS:\")\n",
    "    print(\"-\" * 30)\n",
    "    chi_tests = stat_results['chi_square_tests']\n",
    "    \n",
    "    for test_name, results in chi_tests.items():\n",
    "        if results['significant']:\n",
    "            print(f\"{test_name}: SIGNIFICANT (p={results['p_value']:.4f}, Cram√©r's V={results['cramers_v']:.3f})\")\n",
    "        else:\n",
    "            print(f\"{test_name}: Not significant (p={results['p_value']:.4f})\")\n",
    "\n",
    "# Hypothesis test results\n",
    "if 'hypothesis_tests' in stat_results:\n",
    "    print(\"\\n3. HYPOTHESIS TEST RESULTS:\")\n",
    "    print(\"-\" * 30)\n",
    "    hyp_tests = stat_results['hypothesis_tests']\n",
    "    \n",
    "    if 'gender_gi_awareness' in hyp_tests:\n",
    "        gender_results = hyp_tests['gender_gi_awareness']\n",
    "        print(f\"Male GI Awareness: {gender_results['male_awareness_rate']:.3f}\")\n",
    "        print(f\"Female GI Awareness: {gender_results['female_awareness_rate']:.3f}\")\n",
    "        print(f\"Gender Gap: {gender_results['percentage_gap']:.1f} percentage points\")"

    "plt.subplot(2, 3, 6)\n",
    "scatter = plt.scatter(cleaned_data['Age'], cleaned_data['Years_of_Experience'], \n",
    "                     c=cleaned_data['GI_Aware_Binary'], alpha=0.7, cmap='viridis')\n",
    "plt.xlabel('Age')\n",
    "plt.ylabel('Years of Experience')\n",
    "plt.title('Age vs Experience (Color = GI Aware)')\n",
    "plt.colorbar(scatter, label='GI Aware (1=Yes, 0=No)')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"

    "                                                             y='Ecommerce_Rate', \n",
    "                                                             ax=axes[1,2], s=100)\n",
    "axes[1,2].set_title('GI Awareness vs E-commerce Usage by State')\n",
    "axes[1,2].set_xlabel('GI Awareness Rate')\n",
    "axes[1,2].set_ylabel('E-commerce Usage Rate')\n",
    "\n",
    "# Add state labels to scatter plot\n",
    "for i, state in enumerate(state_analysis.index):\n",
    "    axes[1,2].annotate(state, \n",
    "                      (state_analysis.iloc[i]['GI_Awareness_Rate'], \n",
    "                       state_analysis.iloc[i]['Ecommerce_Rate']),\n",
    "                      xytext=(5, 5), textcoords='offset points', fontsize=8)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# Display state analysis table\n",
    "print(\"STATE-WISE ANALYSIS SUMMARY:\")\n",
    "print(\"=\" * 50)\n",
    "print(state_analysis.round(3))"
